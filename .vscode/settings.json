{
  "solidity.packageDefaultDependenciesContractsDirectory": "src",
  "solidity.packageDefaultDependenciesDirectory": "lib",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "solidity.formatter": "forge",
  "solidity.compileUsingRemoteVersion": "v0.8.17",
  "standard.autoFixOnSave": true,
  "javascript.suggestionActions.enabled": false,
  "tsImportSorter.configuration.development.enableDebug": false,
  "tsImportSorter.configuration.hasSemicolon": false,
  "tsImportSorter.configuration.trailingComma": "none",
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "standard.vscode-standard"
  },
  "cSpell.words": [
    "ARBISCAN",
    "Arbitrum",
    "basegoerli",
    "bignumber",
    "defi",
    "divu",
    "genhtml",
    "IERC",
    "Keccak",
    "lcov",
    "mulu",
    "nomicfoundation",
    "nomiclabs",
    "tokenholder",
    "zerox"
  ],
  "todohighlight.include": [
    "**/*.sol",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "todohighlight.keywords": [
    {
      "text": "revert",
      "color": "#000000",
      "backgroundColor": "#f5bcb3",
      "overviewRulerColor": "#f5bcb3"
    },
    {
      "text": "function initialize",
      "color": "#000000",
      "isWholeLine": true,
      "backgroundColor": "#f7e9d0",
      "overviewRulerColor": "#f7e9d0"
    },
  ]
}